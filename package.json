{
  "name": "wordpress-base-theme",
  "version": "1.0.0",
  "description": "# Tailwind config",
  "main": "index.js",
  "dependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "fs": "^0.0.1-security",
    "postcss-import": "^15.1.0",
    "postcss-import-ext-glob": "^2.0.1",
    "svg-symbol-sprite": "^1.4.1",
    "svgo": "^3.0.2"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.14",
    "browser-sync": "^2.27.7",
    "browser-sync-webpack-plugin": "^2.3.0",
    "cross-env": "^7.0.3",
    "laravel-mix": "^6.0.49",
    "laravel-mix-svg-sprite": "^2.0.0",
    "postcss": "^8.4.25",
    "resolve-url-loader": "^5.0.0",
    "tailwindcss": "^3.3.2",
    "vue-template-compiler": "^2.6.14"
  },
  "scripts": {
    "development": "mix",
    "start": "npm run svgo && npm run svgos && mix watch",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "production": "mix --production",
    "build": "mix --production",
    "svgos": "svg-symbol-sprite -i ./assets/icons/  -o ./assets/images/icons.svg",
    "svgo": "svgo -f ./assets/icons/ -o ./assets/icons/"
  },

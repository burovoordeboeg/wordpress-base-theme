{
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	},
	"rules": {
		"camelcase": [
			1,
			{
				"properties": "never"
			}
		],
		"comma-dangle": 1,
		"curly": 2,
		"no-console": 1,
		"no-alert": 2,
		"quotes": [
			2,
			"single",
			"avoid-escape"
		],
		"no-empty": 2,
		"default-case": 1,
		"linebreak-style": [
			"error",
			"unix"
		],
		"key-spacing": [
			"error",
			{
				"beforeColon": false,
				"afterColon": true
			}
		],
		"no-mixed-spaces-and-tabs": "error",
		"no-trailing-spaces": "error",
		"no-multi-str": "error",
		"no-multiple-empty-lines": 2
	}
}